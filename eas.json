{
  "build": {
    "production": {
      "distribution": "store",
      "releaseChannel": "production",
      "developmentClient": false,
      "env": {
        "API_URL": "https://app.bizmunch.com"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": false,
        "image": "latest"
      }
    },
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
